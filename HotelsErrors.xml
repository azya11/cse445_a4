<Hotels xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://example.com/Hotels.xsd">

	<!-- 1. Missing the required Address attribute "ClosestAirport" -->
	<Hotel Rating="4.2">
		<Name>Renaissance Scottsdale</Name>
		<ContactNumber>480-123-4567</ContactNumber>
		<ContactNumber>480-234-5678</ContactNumber>
		<Address>
			<Number>101</Number>
			<StreetName>East McDowell Rd</StreetName>
			<City>Scottsdale</City>
			<State>AZ</State>
			<PostalCode>85254</PostalCode>
		</Address>
	</Hotel>

	<!-- 2. Missing the required Hotel attribute "Rating" -->
	<Hotel>
		<Name>Hyatt Place Phoenix</Name>
		<ContactNumber>602-345-6789</ContactNumber>
		<Address ClosestAirport="Phoenix Sky Harbor">
			<Number>202</Number>
			<StreetName>North 3rd Ave</StreetName>
			<City>Phoenix</City>
			<State>AZ</State>
			<PostalCode>85004</PostalCode>
		</Address>
	</Hotel>

	<!-- 3. Invalid <Number> (requires integer, but "OneZeroOne" is not an integer) -->
	<Hotel Rating="3.9">
		<Name>Fairfield Inn Tempe</Name>
		<ContactNumber>480-555-1111</ContactNumber>
		<Address ClosestAirport="Phoenix Sky Harbor">
			<Number>OneZeroOne</Number>
			<StreetName>South Mill Ave</StreetName>
			<City>Tempe</City>
			<State>AZ</State>
			<PostalCode>85281</PostalCode>
		</Address>
	</Hotel>

